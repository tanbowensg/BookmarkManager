{
  "manifest_version": 2,

  "name": "bookmarkManager",
  "description": "This extension helps you to manage your bookmarks",
  "version": "1.0",
  "author":"tanbowensg@gmail.com",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webNavigation",
    "bookmarks",
    "https://ajax.googleapis.com/"
  ],

  "background": {
    "page": "bg.html",
    "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webNavigation",
    "https://ajax.googleapis.com/"
  ]
  }
}